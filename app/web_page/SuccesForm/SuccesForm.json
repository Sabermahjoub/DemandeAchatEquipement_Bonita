      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.installation && $data.installation_equipement ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.installation.equipement = $data.installation_equipement;",
        "\treturn {",
        "\t\t//map installation variable to expected task contract input",
        "\t\tinstallationInput: {",
        "\t\t\tequipement: $data.installation.equipement ? {",
        "\t\t\t\tpersistenceId_string: $data.installation.equipement.persistenceId_string !== undefined ? $data.installation.equipement.persistenceId_string : null",

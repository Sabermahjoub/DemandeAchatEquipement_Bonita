      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.fournisseur ){",
        "\treturn {",
        "\t\t//map fournisseur variable to expected task contract input",
        "\t\tfournisseurInput: {",
        "\t\t\tnomFournisseur: $data.fournisseur.nomFournisseur !== undefined ? $data.fournisseur.nomFournisseur : null,",
